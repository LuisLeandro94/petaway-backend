{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2017",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "~v1/*": [
        "app/src/v1/*"
      ],
      "~v1": [
        "app/src/v1"
      ],
      "~models/*": [
        "app/src/models/*",
      ],
      "~models": [
        "app/src/models",
      ],
      "~utils/*": [
        "app/src/utils/*",
      ],
    },
  },
}