{
  "type": "module",
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r esm app/src/server.js",
    "lint": "eslint app/src/** --fix",
    "test": "jest  --coverage --runInBand --forceExit",
    "secure-mode": "jest --watchAll",
    "db:migrate": "sequelize-cli db:migrate",
    "postinstall": "link-module-alias"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "coverageTreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "line": 80
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "23.6.0",
    "sequelize-cli": "^6.3.0",
    "supertest": "3.3.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "consign": "^0.1.6",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "4.16.4",
    "husky": "^7.0.4",
    "jwt-simple": "^0.5.6",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.15",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.9.0",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-auto-migrations-v2": "^1.2.1"
  },
  "_moduleAliases": {
    "~app": "app/src/app.js",
    "~models": "app/src/models",
    "~config": "app/src/config",
    "~v1": "app/src/v1"
  }
}
